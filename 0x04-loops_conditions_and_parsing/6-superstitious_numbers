#!/usr/bin/env bash
# This is a bash script for applying "case"  concept

i=1

while [ "$i" != 21 ]; do
  case $i in
    4)
      echo -e "4\nbad luck from China"
    ;;
  9)
    echo -e "9\nbad luck from Japan"
    ;;
    17)
      echo -e "17\nbad luck from Italy"
      ;;
    *)
      echo "$i"
      ;;
  esac
  i=$((i + 1))
done
